package practice;

import java.util.Scanner;

public class arrStudent {
    Scanner input = new Scanner(System.in);
	Student[] s = new Student[5];
    int i;

    for(i =0; i<s.length;i++) {
    	s[i] = new Student();
    	int id = input.nextInt();
    	double cg = input.nextDouble();
    	s[i].input(id, cg);
    	s[i].display();
    	
    }
}
