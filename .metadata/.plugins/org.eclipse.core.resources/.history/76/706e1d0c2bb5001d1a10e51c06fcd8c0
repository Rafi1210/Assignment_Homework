package practice;

import java.util.Scanner;

public class Student {
	
	int id;
	double cg;
	Student() {
	}
	
	void input(int i , double c) {
		id = i;
	    cg = c;
	}
	void display() {
	System.out.println(id+" "+cg);	
	}

	class main{
	public static void main(String[] args) {
	
		Student rashed  = new Student();
		
		rashed.input(187, 3.84);
		
		rashed.display();
	}
		
	}

}
